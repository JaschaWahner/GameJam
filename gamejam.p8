version 8
__lua__
-- gametest
-- 29.11.17
-- jascha wahner
-- v1
 
 
function _init()
 -- 0 = main menu
 -- 1 = gameplay
 -- 2 = game over
 
 state = 1
 
 frame = 1
 fps   = 30
 sec   = 0
 
 player = {
  x = 64,
  y = 64,
  frames = {
   {1, 2, 3},    -- 1 = left
   {17, 18, 19}, -- 2 = right
   {33, 34, 35}, -- 3 = up 
   {49, 50, 51}, -- 4 = down
  },
  findex = 1,
  ffreq  = 10,
  astate = 2,
  sindex = 0
 }
 
 cam = {
  x = 64,
  y = 64,
  x_min = 64,
  y_min = 64
 }
 
end
 
 
function _update()
 if (state == 0) then
  -- main menu logic
  update_mainmenu()
 
 elseif (state == 1) then
  -- gameplay logic
  update_gameplay()
 
 elseif (state == 2) then
  -- ...
 
 end
end
 
 
function update_mainmenu()
 -- main menu logic
 
end
 
 
function update_gameplay()
 -- gameplay logic
 
 -- sprite index
 player.sindex =
 mget(player.x/8, player.y/8)
 
 -- sprite collision
 if (fget
     (player.sindex) == 1) then
    player.x -= 8,
    player.y -= 8
 end
 
 if (btn(0)) then
  player.x -= 1
  player.astate = 0
 elseif (btn(1)) then
  player.x += 1
  player.astate = 1
 end
 
 if (btn(2)) then
  player.y -= 1
  player.astate = 2
 elseif (btn(3)) then
  player.y += 1
  player.astate = 3
 end
 
 -- poop
 if (btnp(4)) then
  mset(
   player.x/8, -- map tile x
   player.y/8, -- map tile y
   6           -- sprite index
  )
 end 
 
 if (btnp(5)) then
  mset(
   player.x/8,
   player.y/8,
   0
  )
 end
 
 -- player animation
 if (frame%player.ffreq == 0) then
  -- move frame index
  player.findex += 1
  
  -- keep index in bounds
  if (player.findex > #player.frames[1]) then
   player.findex = 1
  end
 end
 
 -- camera motion
  cam.x = player.x
  cam.y = player.y
  
  if (cam.x < cam.x_min) then
   cam.x = cam.x_min
  end
  
  if (cam.y < cam.y_min) then
   cam.y = cam.y_min
  end   
  
  if (frame%fps == 0) then
   sec += 1
  end
  
 frame += 1
 
end
 
 
function _draw()
 cls()
 
 if (state == 0) then
  -- main menu output
  draw_mainmenu()
 
 elseif (state == 1) then
  -- gameplay output
  draw_gameplay()
 
 elseif (state == 2) then
  -- ...
 
 end
end
 
 
function draw_mainmenu()
 -- main menu output
 
end
 
 
function draw_gameplay()
 -- gameplay output
  cls()
 
 rectfill(0, 0, 127, 127, 6)
 
 map(0, 0,
     0, 0,
     128, 64)
 
 
 --[[print("frame : "..frame)
 print("        "..sec.."s")
 print("mod(%): "..frame%fps)]]
 
 -- player
 spr(
  player.frames[player.astate+1][player.findex],
  player.x,
  player.y
 )
 
 -- camera
 camera(cam.x - 64, cam.y - 64)
 
 print (
  player.sindex,
  player.x + 10,
  player.y,
  14
 )
 
 print (
  player.sindex,
  player.x + 10,
  player.y,
  14
 )
end
